<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CadernoVirtual.Primeiro.Medio1.MateriasM1.Historia1"
             BackgroundColor="#7289DA"
             Title="Caderno Virtual 1ºAno" >
    <ContentPage.Content>
        <Frame BackgroundColor="#2c2f33" CornerRadius="10" Margin="10" Padding="10">
            <StackLayout>
                <Label Text="História" HorizontalOptions="Center" FontSize="Title" TextColor="#99AAB5" Margin="55" FontAttributes="Bold"/>
